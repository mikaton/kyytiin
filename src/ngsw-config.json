{
	"index": "/index.html",
	"assetGroups": [{
	  "name": "Kyytiin",
	  "installMode": "prefetch",
	  "resources": {
		"files": [
		  "/favicon.ico",
		  "/index.html"
		],
		"versionedFiles": [
		  "/*.bundle.css",
		  "/*.bundle.js",
		  "/*.chunk.js"
		], 
		"urls": [
			"https://use.fontawesome.com/releases/v5.0.8/css/all.css"
		]
	  }
	}, {
	  "name": "assets",
	  "installMode": "prefetch",
	  "updateMode": "prefetch",
	  "resources": {
		"files": [
		  "/assets/**"
		]
	  }
	}],
	"dataGroups": [
    {
      "name": "api-performance",
      "urls": [
				"localhost:3000/api/user",
				"localhost:3000/api/rides"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "2d"
      }
    }
  ]
  }